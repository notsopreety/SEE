{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/public/(.*)",
        "dest": "/public/$1"
      },
      {
        "src": "/result",
        "dest": "/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/index.html"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }